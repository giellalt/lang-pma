# Name of the speller package in various languages.
# The default is English + autonym:
[speller.name]
en = "@SPELLER_NAME_ENG@"
pma = "@SPELLER_NAME_NATIVE@"

# Description of the speller package in various languages.
# The default is English + autonym:
[speller.description]
en = "@SPELLER_DESC_ENG@"
pma = "@SPELLER_DESC_NATIVE@"

[macos]
system_pkg_id = "no.divvun.MacDivvun.pma"

[windows]
system_product_code = "{3D88E1AC-37C5-48A4-BFF0-583745CA04F6}_is1"

[package.speller]
name = "@GLANGUAGE@"
